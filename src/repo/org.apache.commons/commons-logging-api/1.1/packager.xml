<packager-module version="1.0">
  <property name="name" value="${ivy.packager.module}"/>
  <property name="revision" value="${ivy.packager.revision}"/>
  <property name="organisation" value="commons-logging"/>

  <m2resource
      groupId="${organisation}" artifactId="${name}"
      version="${revision}" repo="https://repo1.maven.org/maven2">

    <artifact tofile="artifacts/jars/${name}.jar"
        sha1="7d4cf5231d46c8524f9b9ed75bb2d1c69ab93322"/>
  </m2resource>
</packager-module>
